msgstr ""
"  SERVER_VERSION_NAME\n"
"  SERVER_VERSION_NUM\n"
"    Serverversion (kurze Zeichenkette oder numerisches Format)\n"

#: help.c:455
msgid ""
"  SHELL_ERROR\n"
"    \"true\" if the last shell command failed, \"false\" if it succeeded\n"
msgstr ""
"  SHELL_ERROR\n"
